{% extends 'base.html.twig' %}
{% form_theme form 'bootstrap_5_layout.html.twig' %}

{% block title %}New User
{% endblock %}

{% block body %}
	{% if form_errors(form) %}
		<div class="alert alert-danger">
			{{ form_errors(form) }}
		</div>
	{% endif %}
	{{ form_start(form,{'attr': {'novalidate': 'novalidate'}}
	) }}
	<!-- ======= Breadcrumbs ======= -->
	<div class="breadcrumbs d-flex align-items-center" style="background-image: url('assets/img/contact-header.jpg');">
		<div class="container position-relative d-flex flex-column align-items-center">

			<h2>S’inscrire</h2>
			<ol>
				<li>
					<a href="index.html">Home</a>
				</li>
				<li>S’inscrire</li>
			</ol>

		</div>
	</div>
	<!-- End Breadcrumbs -->

	{% if form_errors(form) %}
		<div class="alert alert-danger">
			{{ form_errors(form) }}
		</div>
	{% endif %}
	{{ form_start(form,{'attr': {'novalidate': 'novalidate'}}
	) }}
	<section class="contact-form-area section-bg  pt-115 pb-120 fix" src="{{ asset('front/assets/img/baba.jpg') }}" >
	<img src="{{ asset('front/assets/img/baba.jpg') }}" class="background--page" alt="...">

		<div class="container" style="margin-top:-45%;margin-left:25%;">
			<div
				class="row justify-content-end">
				<!-- Contact wrapper -->
				<div class="col-xl-8 col-lg-9">
					<div
						class="contact-form-wrapper">
						<!-- From tittle -->
						<div class="row">
							<div
								class="col-lg-12">
								<!-- Section Tittle -->
								<div class="section-tittle mb-50">
									<span>Get a Qote For Free</span>
									<h2>S’inscrire</h2>
									<p>Rejoignez Amena aujourd’hui.</p>
								</div>
							</div>
						</div>
						<!-- form -->
						<form action="#" class="contact-form">
							<div class="row ">
								<div class="col-lg-6 col-md-6">
									<div class="input-form">
										{{ form_label(form.nom) }}
										{{ form_widget(form.nom) }}
										{{form_errors(form.nom)}}
									</div>
								</div>
								<div class="col-lg-6 col-md-6">
									<div class="input-form">
										<div class="form-group">
											{{ form_label(form.prenom) }}
											{{ form_widget(form.prenom) }}
											{{ form_errors(form.prenom) }}
										</div>
									</div>
								</div>
								<div class="col-lg-12">
									<div class="input-form">
										{{ form_label(form.email) }}
										{{ form_widget(form.email) }}
										{{ form_errors(form.email) }}
										{{ form_label(form.motpass) }}
										{{ form_widget(form.motpass) }}
										{{ form_errors(form.motpass) }}
									</div>
								</div>
								<div class="col-lg-6">
									<div class="select-items">
										<div class="form-group">
											{{ form_label(form.role) }}
											{{ form_widget(form.role) }}
											{{ form_errors(form.role) }}
										</div>
									</div>
								</div>
								<div class="col-lg-6 col-md-6">
									<div class="input-form">
										{{ form_label(form.adress) }}
										{{ form_widget(form.adress) }}
										{{ form_errors(form.adress) }}
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-sm-6">
									<div class="input-form">
										{{ form_label(form.datenaissance) }}
										{{ form_widget(form.datenaissance) }}
										{{ form_errors(form.datenaissance) }}
									</div>
								</div>
								<div class="col-lg-6 col-md-6 col-sm-6">
									<div class="input-form"></div>
								</div>
								<!-- Height Width length -->
								<div class="col-lg-4 col-md-6 col-sm-6">
									<div class="input-form">
										{{ form_label(form.numtel) }}
										{{ form_widget(form.numtel) }}
										{{ form_errors(form.numtel) }}
										{{ form_row(form._token) }}
									</div>
								</div>
								<div class="col-lg-4 col-md-6 col-sm-6">
									<div class="input-form">
										{{ form_label(form.cin) }}
										{{ form_widget(form.cin) }}
										{{ form_errors(form.cin) }}
									</div>
								</div>
								<div class="col-lg-4 col-md-6 col-sm-6">
									<div
										class="input-form">{# <input type="text" placeholder="length"> #}
									</div>
								</div>

								<div class="col-lg-12">
									<button type="submit" class="btn btn-outline-success">Create</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</section>
	<a class="btn btn-outline-dark" href="{{ path('app_user_index') }}">back to list</a>
{% endblock %}
